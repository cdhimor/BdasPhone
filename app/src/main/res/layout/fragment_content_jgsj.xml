    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="桥上净高" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.qiaoLiangXianGaoInfo}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="是否宽路窄桥" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@={viewModel.jgsj.shiFouKuanLuZaiQiao}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="栏杆或防撞护栏" />
        <cn.com.lxsoft.bdasphone.ui.component.EditTextDict
            style="@style/ContentItemTextEdit"
            lx:dict="m1.12"
            android:text="@={viewModel.jgsj.lanGanHuoFangZhuangHuLan}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="中央分隔带" />
        <cn.com.lxsoft.bdasphone.ui.component.EditTextDict
