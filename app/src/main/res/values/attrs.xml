<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EditTextDict">
        <attr name="dict" format="string"></attr>
    </declare-styleable>
    <declare-styleable name="ToolBarBdas">
        <attr name="name" format="string"></attr>
        <attr name="type">
            <enum name="title" value="1"/>
            <enum name="search" value="2"/>
            <enum name="title_back" value="3"/>
        </attr>
        <attr name="init" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="LaybelLayout">
        <attr name="line_padding" format="integer"/>
        <attr name="text_background" format="reference"/><!-- TextView的背景 -->
        <attr name="child_margin" format="integer"/><!-- 子View的外边距 -->
    </declare-styleable>
</resources>
