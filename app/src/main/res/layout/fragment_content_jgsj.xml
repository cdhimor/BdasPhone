            android:text="@={viewModel.jgsj.zhongYangFenGeDai}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="中央或分离带宽度" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.zhongYangFenGeDaiGaoDuInfo}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="引道总宽" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.yinDaoZongKuanInfo}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="引道路面宽" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.yinDaoLuMianKuanInfo}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="引道线性" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.yinDaoXianXing}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="伸缩缝类型" />
        <cn.com.lxsoft.bdasphone.ui.component.EditTextDict
            style="@style/ContentItemTextEdit"
            lx:dict="m2.2"
            android:text="@={viewModel.jgsj.shenSuoFengLeiXing}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="支座类型" />
        <cn.com.lxsoft.bdasphone.ui.component.EditTextDict
            style="@style/ContentItemTextEdit"
            lx:dict="m2.3"
            android:text="@={viewModel.jgsj.zhiZuoLeiXing}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="桥台护坡类型" />
        <cn.com.lxsoft.bdasphone.ui.component.EditTextDict
            style="@style/ContentItemTextEdit"
            lx:dict="m2.3"
            android:text="@={viewModel.jgsj.qiaoTaiHuPoLeiXing}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="调治构造物" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.tiaoZhiGouZaoWu}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="常水位" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.changShuiWeiInfo}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="设计水位" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.sheJiShuiWeiInfo}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="历史洪水位" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.liShiHongShuiWeiInfo}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="墩台防撞设施类型" />
        <cn.com.lxsoft.bdasphone.ui.component.EditTextDict
            style="@style/ContentItemTextEdit"
            lx:dict="m2.5"
            android:text="@={viewModel.jgsj.dunTaiFangZhuangLeiXing}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="是否互通立交" />
        <cn.com.lxsoft.bdasphone.ui.component.EditTextDict
            style="@style/ContentItemTextEdit"
            lx:dict="m1.01"
            android:text="@={viewModel.jgsj.shiFouHuTongLiJiao}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="收费性质" />
        <cn.com.lxsoft.bdasphone.ui.component.EditTextDict
            style="@style/ContentItemTextEdit"
            lx:dict="m2.5"
            android:text="@={viewModel.jgsj.souFeiXingZhi}"/>
    </LinearLayout>
</LinearLayout>
</android.support.v4.widget.NestedScrollView>
</layout>