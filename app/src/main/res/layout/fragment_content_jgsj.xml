<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <import type="cn.com.lxsoft.bdasphone.ui.browse.ContentViewPagerViewModel" />
        <variable
            name="viewModel"
            type="ContentViewPagerViewModel" />
    </data>
<android.support.v4.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:binding="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:lx="http://schemas.android.com/apk/res/cn.com.lxsoft.bdasphone"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="桥梁代码" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@={viewModel.jgsj.qiaoLiangDaiMa}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="桥梁全长" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@={viewModel.jgsj.qiaoLiangQuanChang}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="跨径组合" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@={viewModel.jgsj.qiaoKuanZuHe}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="最大跨径" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.zuiDaKuaJingInfo}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="桥宽组合" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@={viewModel.jgsj.qiaoKuanZuHe}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="桥面净宽" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.qiaoMianJingKuanInfo}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="桥面全宽" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.qiaoMianQuanKuanInfo}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="桥面标高" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.qiaoGaoInfo}"/>
    </LinearLayout>
    <LinearLayout style="@style/ContentItem">
        <TextView
            style="@style/ContentItemTextView"
            android:text="桥下净高" />
        <EditText
            style="@style/ContentItemTextEdit"
            android:text="@{viewModel.jgsj.jingGaoInfo}"/>
